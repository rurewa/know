/ Мигаем светодиодом с помощью оператора цикла for

void setup()
{
  pinMode(13, OUTPUT);
}

void loop()
{
  for (int i=50; i<=500; i=i)   // Где i=i это i=i+0
  {
    digitalWrite(13, 1);
    delay(i); // задержка (берём значение из i)
    digitalWrite(13, 0);
    delay(i); // задержка (берём значение из i)
  }
}
